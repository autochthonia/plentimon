{
  "name": "plentimon",
  "version": "0.0.1",
  "description": "A dicebot for Exalted based loosely on an original by @strass",
  "main": "lib/plentimon-dice.js",
  "types": "lib/types/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts\" --source-maps inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/autochthonia/plentimon.git"
  },
  "keywords": [
    "exalted3",
    "typescript",
    "babel",
    "aws-sdk-v3"
  ],
  "author": "autochthonia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/autochthonia/plentimon/issues"
  },
  "homepage": "https://github.com/autochthonia/plentimon#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/lodash": "^4.14.126",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  }
